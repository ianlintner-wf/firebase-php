{
    "name": "ianlintner/firebase-php-legacy",
    "description": "Firebase Admin SDK Legacy",
    "keywords": ["firebase", "google", "sdk", "api", "database"],
    "homepage": "https://github.com/ianlintner-wf/firebase-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérôme Gamez",
            "homepage": "https://github.com/jeromegamez"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "fig/http-message-util": "^1.1",
        "google/auth": "^0.11.0|^1.0",
        "guzzlehttp/guzzle": "^5.3.1|~6.0",
        "ianlintner/firebase-tokens-legacy": "master",
        "lcobucci/jwt": "^3.2",
        "mtdowling/jmespath.php": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Kreait\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kreait\\Tests\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.0"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
    "repositories": [
    {
      "type": "vcs",
      "url":  "git@github.com:ianlintner-wf/firebase-tokens-php.git"
    }
}
